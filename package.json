{
  "name": "@gen-tech/scroll-manager",
  "version": "0.0.0",
  "description": "Improved Scroll Events & Scroll Management",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://gen-tech.github.io/scroll-manager",
  "scripts": {
    "prepublishOnly": "tsc",
    "document": "typedoc --out docs --excludeNotExported --excludePrivate --disableOutputCheck --gitRevision master",
    "prepare-github-pages": "npm run document && echo \"\" > ./docs/.nojekyll",
    "test": "karma start ./karma.conf.js --single-run",
    "test:debug": "karma start",
    "cover": "cat ./coverage/lcovonly | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gen-tech/scroll-manager.git"
  },
  "keywords": [
    "browser",
    "scroll",
    "manager",
    "events",
    "observer",
    "observable"
  ],
  "author": "alisahinozcelik@gmail.com",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/gen-tech/scroll-manager/issues"
  },
  "dependencies": {
    "platform": "^1.3.5",
    "rxjs": "^6.3.3",
    "smoothscroll-polyfill": "^0.4.3",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@gen-tech/version-update": "^0.1.2",
    "@types/jasmine": "^2.8.11",
    "@types/platform": "^1.3.1",
    "coveralls": "^3.0.2",
    "jasmine": "^3.3.0",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.0",
    "karma-typescript": "3.0.13",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.6"
  },
  "directories": {
    "doc": "docs"
  }
}
